er":{"interfaceLanguage":"swift","url":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext\/startSafariAppRedirectFlow()"},"hierarchy":{"paths":[["doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI","doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext"]]},"references":{"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI":{"title":"StripePaymentsUI","kind":"symbol","type":"topic","identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI","role":"collection","url":"\/documentation\/stripepaymentsui","abstract":[{"type":"text","text":"The StripePaymentsUI module contains UI elements and API bindings for building a custom payment flow using Stripe."}]},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI/STPRedirectContext/startSafariAppRedirectFlow()":{"identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext\/startSafariAppRedirectFlow()","title":"startSafariAppRedirectFlow()","role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"text":"startSafariAppRedirectFlow","kind":"identifier"},{"text":"()","kind":"text"}],"url":"\/documentation\/stripepaymentsui\/stpredirectcontext\/startsafariappredirectflow()","type":"topic","abstract":[{"type":"text","text":"Starts a redirect flow by calling "},{"code":"openURL","type":"codeVoice"},{"text":" to bounce the user out to","type":"text"},{"text":" ","type":"text"},{"text":"the Safari app.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"The context will listen for app open notifications and fire its completion"},{"text":" ","type":"text"},{"type":"text","text":"block the next time the user re-opens the app (either manually or via url)"}],"kind":"symbol"},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI/STPRedirectContext":{"title":"STPRedirectContext","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPRedirectContext"}],"url":"\/documentation\/stripepaymentsui\/stpredirectcontext","kind":"symbol","abstract":[{"type":"text","text":"This is a helper class for handling redirects associated with STPSource and"},{"text":" ","type":"text"},{"type":"text","text":"STPPaymentIntents."},{"text":" ","type":"text"},{"text":"Init and retain an instance with the Source or PaymentIntent you want to handle,","type":"text"},{"text":" ","type":"text"},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"type":"text","text":" "},{"type":"text","text":"when the redirect completes."},{"text":" ","type":"text"},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"type":"text","text":" "},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"text":" ","type":"text"},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"type":"text","text":" "},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"text":" ","type":"text"},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"type":"text","text":" "},{"text":"However, it is possible the when the redirect is “completed”, the user may","type":"text"},{"type":"text","text":" "},{"type":"text","text":"have not actually completed the necessary actions to authorize the charge."},{"text":" ","type":"text"},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"text":", as a way","type":"text"},{"text":" ","type":"text"},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"type":"topic","identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext","navigatorTitle":[{"kind":"identifier","text":"STPRedirectContext"}],"role":"symbol"}}}