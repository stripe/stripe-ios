{"variants":[{"paths":["\/documentation\/stripepaymentsui\/stpredirectcontext\/stpredirectcontexterrordomain"],"traits":[{"interfaceLanguage":"swift"}]}],"sections":[],"primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"tokens":[{"kind":"attribute","text":"@"},{"kind":"attribute","text":"MainActor","preciseIdentifier":"s:ScM"},{"kind":"text","text":" "},{"kind":"attribute","text":"@objc"},{"text":" ","kind":"text"},{"text":"static","kind":"keyword"},{"kind":"text","text":" "},{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPRedirectContextErrorDomain"},{"kind":"text","text":": "},{"preciseIdentifier":"s:SS","kind":"typeIdentifier","text":"String"}],"platforms":["iOS"]}]}],"schemaVersion":{"minor":3,"patch":0,"major":0},"kind":"symbol","abstract":[{"text":"The domain for NSErrors specific to ","type":"text"},{"type":"codeVoice","code":"STPRedirectContext"}],"hierarchy":{"paths":[["doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI","doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext"]]},"identifier":{"url":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext\/STPRedirectContextErrorDomain","interfaceLanguage":"swift"},"metadata":{"role":"symbol","modules":[{"name":"StripePaymentsUI"}],"roleHeading":"Type Property","externalID":"c:@M@StripePayments@objc(cs)STPRedirectContext(cpy)STPRedirectContextErrorDomain","title":"STPRedirectContextErrorDomain","symbolKind":"property","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPRedirectContextErrorDomain","kind":"identifier"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"}]},"references":{"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI/STPRedirectContext":{"url":"\/documentation\/stripepaymentsui\/stpredirectcontext","title":"STPRedirectContext","kind":"symbol","navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"type":"topic","abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"text":"Init and retain an instance with the Source or PaymentIntent you want to handle,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"then choose a redirect method. The context will fire the completion handler"},{"type":"text","text":" "},{"type":"text","text":"when the redirect completes."},{"text":" ","type":"text"},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"type":"text","text":" "},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"text":" ","type":"text"},{"text":"or the sandboxed SFSafariViewController class. The context attempts to","type":"text"},{"type":"text","text":" "},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"for both app foregrounds and url callbacks received in the app delegate."},{"text":" ","type":"text"},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"type":"text","text":" "},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"You should not use either this class, nor "},{"code":"STPAPIClient","type":"codeVoice"},{"text":", as a way","type":"text"},{"type":"text","text":" "},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"text":"STPRedirectContext","kind":"identifier"}],"identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext"},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI/STPRedirectContext/STPRedirectContextErrorDomain":{"url":"\/documentation\/stripepaymentsui\/stpredirectcontext\/stpredirectcontexterrordomain","fragments":[{"text":"static","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPRedirectContextErrorDomain"},{"kind":"text","text":": "},{"text":"String","kind":"typeIdentifier","preciseIdentifier":"s:SS"}],"type":"topic","title":"STPRedirectContextErrorDomain","kind":"symbol","abstract":[{"type":"text","text":"The domain for NSErrors specific to "},{"code":"STPRedirectContext","type":"codeVoice"}],"identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext\/STPRedirectContextErrorDomain","role":"symbol"},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI":{"type":"topic","url":"\/documentation\/stripepaymentsui","title":"StripePaymentsUI","identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI","abstract":[{"text":"The StripePaymentsUI module contains UI elements and API bindings for building a custom payment flow using Stripe.","type":"text"}],"kind":"symbol","role":"collection"}}}