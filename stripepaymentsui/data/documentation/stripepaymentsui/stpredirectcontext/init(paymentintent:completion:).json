i\/documentation\/StripePaymentsUI\/STPPaymentIntent","url":"\/documentation\/stripepaymentsui\/stppaymentintent","navigatorTitle":[{"kind":"identifier","text":"STPPaymentIntent"}],"fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPPaymentIntent"}],"kind":"symbol"},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI/STPRedirectContextPaymentIntentCompletionBlock":{"navigatorTitle":[{"text":"STPRedirectContextPaymentIntentCompletionBlock","kind":"identifier"}],"identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContextPaymentIntentCompletionBlock","kind":"symbol","type":"topic","url":"\/documentation\/stripepaymentsui\/stpredirectcontextpaymentintentcompletionblock","role":"symbol","fragments":[{"text":"typealias","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPRedirectContextPaymentIntentCompletionBlock"}],"abstract":[{"type":"text","text":"A callback that is executed when the context believes the redirect action has been completed."}],"title":"STPRedirectContextPaymentIntentCompletionBlock"},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI/STPRedirectContext":{"kind":"symbol","url":"\/documentation\/stripepaymentsui\/stpredirectcontext","role":"symbol","title":"STPRedirectContext","identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI\/STPRedirectContext","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPRedirectContext","kind":"identifier"}],"abstract":[{"type":"text","text":"This is a helper class for handling redirects associated with STPSource and"},{"text":" ","type":"text"},{"type":"text","text":"STPPaymentIntents."},{"text":" ","type":"text"},{"text":"Init and retain an instance with the Source or PaymentIntent you want to handle,","type":"text"},{"type":"text","text":" "},{"type":"text","text":"then choose a redirect method. The context will fire the completion handler"},{"text":" ","type":"text"},{"type":"text","text":"when the redirect completes."},{"text":" ","type":"text"},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"type":"text","text":" "},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"type":"text","text":" "},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"type":"text","text":" "},{"type":"text","text":"detect when the user has completed the necessary redirect action by listening"},{"type":"text","text":" "},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"type":"text","text":" "},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"type":"text","text":" "},{"text":"You should not use either this class, nor ","type":"text"},{"code":"STPAPIClient","type":"codeVoice"},{"text":", as a way","type":"text"},{"type":"text","text":" "},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"type":"text","text":" "},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"text":" ","type":"text"},{"text":"state changes and to make the charge.","type":"text"}],"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"type":"topic"},"doc://com.stripe.stripe-payments-ui/documentation/StripePaymentsUI":{"identifier":"doc:\/\/com.stripe.stripe-payments-ui\/documentation\/StripePaymentsUI","type":"topic","abstract":[{"type":"text","text":"The StripePaymentsUI module contains UI elements and API bindings for building a custom payment flow using Stripe."}],"title":"StripePaymentsUI","url":"\/documentation\/stripepaymentsui","role":"collection","kind":"symbol"}}}
