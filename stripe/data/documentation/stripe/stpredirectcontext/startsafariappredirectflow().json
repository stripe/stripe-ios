{"sections":[],"identifier":{"url":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/startSafariAppRedirectFlow()","interfaceLanguage":"swift"},"metadata":{"title":"startSafariAppRedirectFlow()","roleHeading":"Instance Method","symbolKind":"method","role":"symbol","externalID":"c:@M@StripePayments@objc(cs)STPRedirectContext(im)startSafariAppRedirectFlow","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"startSafariAppRedirectFlow"},{"text":"()","kind":"text"}],"modules":[{"name":"Stripe"}]},"primaryContentSections":[{"declarations":[{"tokens":[{"text":"@","kind":"attribute"},{"preciseIdentifier":"s:ScM","text":"MainActor","kind":"attribute"},{"kind":"text","text":" "},{"text":"@objc","kind":"attribute"},{"kind":"text","text":" "},{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"startSafariAppRedirectFlow","kind":"identifier"},{"text":"()","kind":"text"}],"languages":["swift"],"platforms":["iOS"]}],"kind":"declarations"},{"content":[{"type":"heading","text":"Discussion","anchor":"discussion","level":2},{"type":"aside","name":"Note","content":[{"inlineContent":[{"type":"text","text":"This method does nothing if the context is not in the"},{"text":" ","type":"text"},{"type":"codeVoice","code":"STPRedirectContextStateNotStarted"},{"type":"text","text":" state."}],"type":"paragraph"}],"style":"note"}],"kind":"content"}],"abstract":[{"type":"text","text":"Starts a redirect flow by calling "},{"code":"openURL","type":"codeVoice"},{"type":"text","text":" to bounce the user out to"},{"text":" ","type":"text"},{"type":"text","text":"the Safari app."},{"type":"text","text":" "},{"text":"The context will listen for app open notifications and fire its completion","type":"text"},{"text":" ","type":"text"},{"text":"block the next time the user re-opens the app (either manually or via url)","type":"text"}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/stripe\/stpredirectcontext\/startsafariappredirectflow()"]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"kind":"symbol","hierarchy":{"paths":[["doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext"]]},"references":{"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"title":"STPRedirectContext","url":"\/documentation\/stripe\/stpredirectcontext","abstract":[{"type":"text","text":"This is a helper class for handling redirects associated with STPSource and"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"type":"text","text":" "},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"when the redirect completes."},{"type":"text","text":" "},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"text":" ","type":"text"},{"text":"during this process, as all actions take place in either the Safari app","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"text":" ","type":"text"},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"type":"text","text":" "},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"type":"text","text":" "},{"text":"However, it is possible the when the redirect is “completed”, the user may","type":"text"},{"type":"text","text":" "},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"text":" ","type":"text"},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"type":"text","text":", as a way"},{"text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"text":" ","type":"text"},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"type":"topic","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPRedirectContext"}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","role":"symbol","kind":"symbol"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/startSafariAppRedirectFlow()":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/startSafariAppRedirectFlow()","type":"topic","kind":"symbol","abstract":[{"type":"text","text":"Starts a redirect flow by calling "},{"type":"codeVoice","code":"openURL"},{"text":" to bounce the user out to","type":"text"},{"type":"text","text":" "},{"text":"the Safari app.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"The context will listen for app open notifications and fire its completion"},{"type":"text","text":" "},{"text":"block the next time the user re-opens the app (either manually or via url)","type":"text"}],"role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"text":"startSafariAppRedirectFlow","kind":"identifier"},{"text":"()","kind":"text"}],"title":"startSafariAppRedirectFlow()","url":"\/documentation\/stripe\/stpredirectcontext\/startsafariappredirectflow()"},"doc://com.stripe.stripe-ios/documentation/Stripe":{"abstract":[],"kind":"symbol","url":"\/documentation\/stripe","title":"Stripe","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","role":"collection","type":"topic"}}}