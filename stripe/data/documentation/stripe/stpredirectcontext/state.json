r may"},{"type":"text","text":" "},{"type":"text","text":"have not actually completed the necessary actions to authorize the charge."},{"type":"text","text":" "},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"text":", as a way","type":"text"},{"type":"text","text":" "},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"text":" ","type":"text"},{"type":"text","text":"state changes and to make the charge."}],"type":"topic","url":"\/documentation\/stripe\/stpredirectcontext","kind":"symbol"},"doc://com.stripe.stripe-ios/documentation/Stripe":{"title":"Stripe","url":"\/documentation\/stripe","type":"topic","kind":"symbol","abstract":[],"role":"collection","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/state":{"url":"\/documentation\/stripe\/stpredirectcontext\/state","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/state","title":"state","type":"topic","role":"symbol","fragments":[{"text":"var","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"state"},{"kind":"text","text":": "},{"text":"STPRedirectContextState","preciseIdentifier":"c:@M@StripePayments@E@STPRedirectContextState","kind":"typeIdentifier"}],"kind":"symbol","abstract":[{"type":"text","text":"The current state of the context."}]}}}