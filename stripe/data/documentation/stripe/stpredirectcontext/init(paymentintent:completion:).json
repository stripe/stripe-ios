text":" ","type":"text"},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"type":"text","text":" "},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPRedirectContext"}],"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"title":"STPRedirectContext","kind":"symbol","url":"\/documentation\/stripe\/stpredirectcontext","type":"topic"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContextPaymentIntentCompletionBlock":{"abstract":[{"type":"text","text":"A callback that is executed when the context believes the redirect action has been completed."}],"role":"symbol","type":"topic","title":"STPRedirectContextPaymentIntentCompletionBlock","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContextPaymentIntentCompletionBlock","url":"\/documentation\/stripe\/stpredirectcontextpaymentintentcompletionblock","fragments":[{"kind":"keyword","text":"typealias"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPRedirectContextPaymentIntentCompletionBlock"}],"kind":"symbol","navigatorTitle":[{"text":"STPRedirectContextPaymentIntentCompletionBlock","kind":"identifier"}]}}}