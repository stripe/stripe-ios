text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"state changes and to make the charge."}],"url":"\/documentation\/stripe\/stpredirectcontext","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","title":"STPRedirectContext","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPRedirectContext"}],"type":"topic","kind":"symbol","navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}]},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContextPaymentIntentCompletionBlock":{"navigatorTitle":[{"text":"STPRedirectContextPaymentIntentCompletionBlock","kind":"identifier"}],"abstract":[{"type":"text","text":"A callback that is executed when the context believes the redirect action has been completed."}],"kind":"symbol","url":"\/documentation\/stripe\/stpredirectcontextpaymentintentcompletionblock","title":"STPRedirectContextPaymentIntentCompletionBlock","type":"topic","role":"symbol","fragments":[{"text":"typealias","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPRedirectContextPaymentIntentCompletionBlock","kind":"identifier"}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContextPaymentIntentCompletionBlock"}}}