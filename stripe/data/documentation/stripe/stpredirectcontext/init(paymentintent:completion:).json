text":", as a way"},{"type":"text","text":" "},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"state changes and to make the charge."}]},"doc://com.stripe.stripe-ios/documentation/Stripe/STPPaymentIntent":{"kind":"symbol","title":"STPPaymentIntent","url":"\/documentation\/stripe\/stppaymentintent","abstract":[{"type":"text","text":"A PaymentIntent tracks the process of collecting a payment from your customer."}],"role":"symbol","fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"text":"STPPaymentIntent","kind":"identifier"}],"type":"topic","navigatorTitle":[{"kind":"identifier","text":"STPPaymentIntent"}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPPaymentIntent"},"doc://com.stripe.stripe-ios/documentation/Stripe":{"url":"\/documentation\/stripe","title":"Stripe","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","kind":"symbol","abstract":[],"type":"topic","role":"collection"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContextPaymentIntentCompletionBlock":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContextPaymentIntentCompletionBlock","kind":"symbol","role":"symbol","url":"\/documentation\/stripe\/stpredirectcontextpaymentintentcompletionblock","navigatorTitle":[{"kind":"identifier","text":"STPRedirectContextPaymentIntentCompletionBlock"}],"title":"STPRedirectContextPaymentIntentCompletionBlock","abstract":[{"text":"A callback that is executed when the context believes the redirect action has been completed.","type":"text"}],"fragments":[{"text":"typealias","kind":"keyword"},{"kind":"text","text":" "},{"text":"STPRedirectContextPaymentIntentCompletionBlock","kind":"identifier"}],"type":"topic"},"doc://com.stripe