ext(im)safariViewController:initialLoadDidRedirectToURL:","role":"symbol","roleHeading":"Instance Method","modules":[{"name":"Stripe","relatedModules":["StripePayments"]}],"fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"safariViewController","kind":"identifier"},{"kind":"text","text":"("},{"kind":"typeIdentifier","text":"SFSafariViewController","preciseIdentifier":"c:objc(cs)SFSafariViewController"},{"text":", ","kind":"text"},{"text":"initialLoadDidRedirectTo","kind":"externalParam"},{"kind":"text","text":": "},{"text":"URL","kind":"typeIdentifier","preciseIdentifier":"s:10Foundation3URLV"},{"kind":"text","text":")"}]},"references":{"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/safariViewController(_:initialLoadDidRedirectTo:)":{"abstract":[],"title":"safariViewController(_:initialLoadDidRedirectTo:)","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/safariViewController(_:initialLoadDidRedirectTo:)","type":"topic","url":"\/documentation\/stripe\/stpredirectcontext\/safariviewcontroller(_:initialloaddidredirectto:)","role":"symbol","kind":"symbol","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"safariViewController"},{"text":"(","kind":"text"},{"text":"SFSafariViewController","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)SFSafariViewController"},{"kind":"text","text":", "},{"kind":"externalParam","text":"initialLoadDidRedirectTo"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:10Foundation3URLV","text":"URL","kind":"typeIdentifier"},{"text":")","kind":"text"}]},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/SFSafariViewControllerDelegate-Implementations":{"title":"SFSafariViewControllerDelegate Implementations","type":"topic","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/SFSafariViewControllerDelegate-Implementations","role":"collectionGroup","kind":"article","url":"\/documentation\/stripe\/stpredirectcontext\/sfsafariviewcontrollerdelegate-implementations","abstract":[]},"doc://com.stripe.stripe-ios/documentation/Stripe":{"title":"Stripe","type":"topic","kind":"symbol","role":"collection","url":"\/documentation\/stripe","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","abstract":[]},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPRedirectContext"}],"url":"\/documentation\/stripe\/stpredirectcontext","abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"type":"text","text":" "},{"text":"STPPaymentIntents.","type":"text"},{"type":"text","text":" "},{"text":"Init and retain an instance with the Source or PaymentIntent you want to handle,","type":"text"},{"type":"text","text":" "},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"type":"text","text":" "},{"text":"when the redirect completes.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"Due to the nature of iOS, very little concrete information can be gained"},{"text":" ","type":"text"},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"text":" ","type":"text"},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"type":"text","text":" "},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"type":"text","text":" "},{"type":"text","text":"for both app foregrounds and url callbacks received in the app delegate."},{"text":" ","type":"text"},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"text":" ","type":"text"},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"You should not use either this class, nor "},{"code":"STPAPIClient","type":"codeVoice"},{"text":", as a way","type":"text"},{"type":"text","text":" "},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"type":"text","text":" "},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"type":"topic","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","kind":"symbol","title":"STPRedirectContext","role":"symbol"}}}