les":["StripePayments"],"name":"Stripe"}],"externalID":"c:@CM@StripePayments@objc(cs)STPRedirectContext(im)safariViewControllerDidFinish:"},"references":{"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/safariViewControllerDidFinish(_:)":{"kind":"symbol","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/safariViewControllerDidFinish(_:)","url":"\/documentation\/stripe\/stpredirectcontext\/safariviewcontrollerdidfinish(_:)","title":"safariViewControllerDidFinish(_:)","role":"symbol","abstract":[],"fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"safariViewControllerDidFinish"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)SFSafariViewController","text":"SFSafariViewController"},{"text":")","kind":"text"}],"type":"topic"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/SFSafariViewControllerDelegate-Implementations":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/SFSafariViewControllerDelegate-Implementations","kind":"article","role":"collectionGroup","abstract":[],"type":"topic","title":"SFSafariViewControllerDelegate Implementations","url":"\/documentation\/stripe\/stpredirectcontext\/sfsafariviewcontrollerdelegate-implementations"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"STPPaymentIntents."},{"text":" ","type":"text"},{"text":"Init and retain an instance with the Source or PaymentIntent you want to handle,","type":"text"},{"type":"text","text":" "},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"when the redirect completes."},{"type":"text","text":" "},{"type":"text","text":"Due to the nature of iOS, very little concrete information can be gained"},{"text":" ","type":"text"},{"text":"during this process, as all actions take place in either the Safari app","type":"text"},{"text":" ","type":"text"},{"text":"or the sandboxed SFSafariViewController class. The context attempts to","type":"text"},{"type":"text","text":" "},{"type":"text","text":"detect when the user has completed the necessary redirect action by listening"},{"type":"text","text":" "},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"text":" ","type":"text"},{"text":"However, it is possible the when the redirect is “completed”, the user may","type":"text"},{"text":" ","type":"text"},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"text":" ","type":"text"},{"text":"You should not use either this class, nor ","type":"text"},{"code":"STPAPIClient","type":"codeVoice"},{"text":", as a way","type":"text"},{"text":" ","type":"text"},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"type":"text","text":" "},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"title":"STPRedirectContext","fragments":[{"text":"class","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"STPRedirectContext"}],"url":"\/documentation\/stripe\/stpredirectcontext","type":"topic","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","role":"symbol","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"STPRedirectContext"}]},"doc://com.stripe.stripe-ios/documentation/Stripe":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","abstract":[],"url":"\/documentation\/stripe","kind":"symbol","title":"Stripe","type":"topic","role":"collection"}}}