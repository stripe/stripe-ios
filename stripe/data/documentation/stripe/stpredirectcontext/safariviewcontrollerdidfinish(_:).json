ts":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/stripe\/stpredirectcontext\/safariviewcontrollerdidfinish(_:)"]}],"references":{"doc://com.stripe.stripe-ios/documentation/Stripe":{"title":"Stripe","url":"\/documentation\/stripe","type":"topic","kind":"symbol","abstract":[],"role":"collection","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/safariViewControllerDidFinish(_:)":{"abstract":[],"type":"topic","url":"\/documentation\/stripe\/stpredirectcontext\/safariviewcontrollerdidfinish(_:)","kind":"symbol","role":"symbol","fragments":[{"text":"func","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"safariViewControllerDidFinish"},{"text":"(","kind":"text"},{"text":"SFSafariViewController","kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)SFSafariViewController"},{"text":")","kind":"text"}],"title":"safariViewControllerDidFinish(_:)","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/safariViewControllerDidFinish(_:)"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/SFSafariViewControllerDelegate-Implementations":{"url":"\/documentation\/stripe\/stpredirectcontext\/sfsafariviewcontrollerdelegate-implementations","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/SFSafariViewControllerDelegate-Implementations","title":"SFSafariViewControllerDelegate Implementations","type":"topic","role":"collectionGroup","kind":"article","abstract":[]},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"title":"STPRedirectContext","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPRedirectContext","kind":"identifier"}],"navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}],"role":"symbol","abstract":[{"type":"text","text":"This is a helper class for handling redirects associated with STPSource and"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"text":" ","type":"text"},{"type":"text","text":"then choose a redirect method. The context will fire the completion handler"},{"text":" ","type":"text"},{"text":"when the redirect completes.","type":"text"},{"type":"text","text":" "},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"type":"text","text":" "},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"type":"text","text":" "},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"type":"text","text":" "},{"type":"text","text":"detect when the user has completed the necessary redirect action by listening"},{"text":" ","type":"text"},{"type":"text","text":"for both app foregrounds and url callbacks received in the app delegate."},{"text":" ","type":"text"},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"type":"text","text":" "},{"type":"text","text":"have not actually completed the necessary actions to authorize the charge."},{"type":"text","text":" "},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"text":", as a way","type":"text"},{"type":"text","text":" "},{"text":"to determine when you should charge the Source or to determine if the redirect","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"text":" ","type":"text"},{"type":"text","text":"state changes and to make the charge."}],"type":"topic","url":"\/documentation\/stripe\/stpredirectcontext","kind":"symbol"}}}