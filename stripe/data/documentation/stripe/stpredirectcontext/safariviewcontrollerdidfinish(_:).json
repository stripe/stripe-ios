larations"}],"references":{"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/safariViewControllerDidFinish(_:)":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/safariViewControllerDidFinish(_:)","fragments":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"safariViewControllerDidFinish","kind":"identifier"},{"text":"(","kind":"text"},{"text":"SFSafariViewController","preciseIdentifier":"c:objc(cs)SFSafariViewController","kind":"typeIdentifier"},{"kind":"text","text":")"}],"title":"safariViewControllerDidFinish(_:)","type":"topic","abstract":[],"kind":"symbol","role":"symbol","url":"\/documentation\/stripe\/stpredirectcontext\/safariviewcontrollerdidfinish(_:)"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/SFSafariViewControllerDelegate-Implementations":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/SFSafariViewControllerDelegate-Implementations","title":"SFSafariViewControllerDelegate Implementations","type":"topic","abstract":[],"kind":"article","role":"collectionGroup","url":"\/documentation\/stripe\/stpredirectcontext\/sfsafariviewcontrollerdelegate-implementations"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"abstract":[{"type":"text","text":"This is a helper class for handling redirects associated with STPSource and"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"text":" ","type":"text"},{"text":"then choose a redirect method. The context will fire the completion handler","type":"text"},{"type":"text","text":" "},{"text":"when the redirect completes.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Due to the nature of iOS, very little concrete information can be gained"},{"type":"text","text":" "},{"text":"during this process, as all actions take place in either the Safari app","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"text":" ","type":"text"},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"for both app foregrounds and url callbacks received in the app delegate."},{"text":" ","type":"text"},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"text":" ","type":"text"},{"type":"text","text":"have not actually completed the necessary actions to authorize the charge."},{"type":"text","text":" "},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"type":"text","text":", as a way"},{"text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"text":" ","type":"text"},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"text":"STPRedirectContext","kind":"identifier"}],"kind":"symbol","url":"\/documentation\/stripe\/stpredirectcontext","type":"topic","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","title":"STPRedirectContext","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"STPRedirectContext"}]},"doc://com.stripe.stripe-ios/documentation/Stripe":{"kind":"symbol","abstract":[],"title":"Stripe","url":"\/documentation\/stripe","role":"collection","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","type":"topic"}}}
