ts":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/stripe\/stpredirectcontext\/safariviewcontrollerdidfinish(_:)"]}],"references":{"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/SFSafariViewControllerDelegate-Implementations":{"kind":"article","url":"\/documentation\/stripe\/stpredirectcontext\/sfsafariviewcontrollerdelegate-implementations","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/SFSafariViewControllerDelegate-Implementations","role":"collectionGroup","abstract":[],"title":"SFSafariViewControllerDelegate Implementations","type":"topic"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/safariViewControllerDidFinish(_:)":{"kind":"symbol","role":"symbol","fragments":[{"kind":"keyword","text":"func"},{"text":" ","kind":"text"},{"kind":"identifier","text":"safariViewControllerDidFinish"},{"text":"(","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"c:objc(cs)SFSafariViewController","text":"SFSafariViewController"},{"text":")","kind":"text"}],"type":"topic","abstract":[],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/safariViewControllerDidFinish(_:)","title":"safariViewControllerDidFinish(_:)","url":"\/documentation\/stripe\/stpredirectcontext\/safariviewcontrollerdidfinish(_:)"},"doc://com.stripe.stripe-ios/documentation/Stripe":{"abstract":[],"kind":"symbol","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","title":"Stripe","type":"topic","role":"collection","url":"\/documentation\/stripe"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"text":" ","type":"text"},{"type":"text","text":"then choose a redirect method. The context will fire the completion handler"},{"text":" ","type":"text"},{"text":"when the redirect completes.","type":"text"},{"type":"text","text":" "},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"text":" ","type":"text"},{"text":"during this process, as all actions take place in either the Safari app","type":"text"},{"text":" ","type":"text"},{"text":"or the sandboxed SFSafariViewController class. The context attempts to","type":"text"},{"type":"text","text":" "},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"type":"text","text":" "},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"text":" ","type":"text"},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"You should not use either this class, nor "},{"type":"codeVoice","code":"STPAPIClient"},{"text":", as a way","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"state changes and to make the charge."}],"url":"\/documentation\/stripe\/stpredirectcontext","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","title":"STPRedirectContext","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPRedirectContext"}],"type":"topic","kind":"symbol","navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}]}}}