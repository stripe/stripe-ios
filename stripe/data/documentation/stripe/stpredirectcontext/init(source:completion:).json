tifier","text":"STPSource","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPSource","preciseIdentifier":"c:@M@StripePayments@objc(cs)STPSource"},{"kind":"text","text":", "},{"kind":"externalParam","text":"completion"},{"text":": ","kind":"text"},{"text":"@escaping","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPRedirectContextSourceCompletionBlock","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContextSourceCompletionBlock","preciseIdentifier":"s:14StripePayments39STPRedirectContextSourceCompletionBlocka","kind":"typeIdentifier"},{"kind":"text","text":")"}],"languages":["swift"]}]},{"content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"type":"aside","content":[{"inlineContent":[{"text":"You must ensure that the returnURL set up in the created source","type":"text"},{"type":"text","text":" "},{"text":"correctly goes to your app so that users can be returned once","type":"text"},{"type":"text","text":" "},{"text":"they complete the redirect in the web broswer.","type":"text"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"inlineContent":[{"type":"text","text":"Parameters:"}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"source: The source that needs user redirect action to be taken.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"completion: A block to fire when the action is believed to have","type":"text"},{"text":" ","type":"text"},{"text":"been completed.","type":"text"}]}]}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"Returns: nil if the specified source is not a redirect-flow source. Otherwise","type":"text"},{"text":" ","type":"text"},{"text":"a new context object.","type":"text"}]}]}]}],"style":"note","name":"Note"},{"name":"Note","style":"note","content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Execution of the completion block does not necessarily mean the user"},{"type":"text","text":" "},{"type":"text","text":"successfully performed the redirect action. You should listen for source status"},{"type":"text","text":" "},{"text":"change webhooks on your backend to determine the result of a redirect.","type":"text"}]}],"type":"aside"}],"kind":"content"}],"references":{"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext/init(source:completion:)":{"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext\/init(source:completion:)","fragments":[{"kind":"keyword","text":"convenience"},{"kind":"text","text":" "},{"text":"init","kind":"identifier"},{"text":"?(","kind":"text"},{"text":"source","kind":"externalParam"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","text":"STPSource","preciseIdentifier":"c:@M@StripePayments@objc(cs)STPSource"},{"kind":"text","text":", "},{"kind":"externalParam","text":"completion"},{"text":": ","kind":"text"},{"kind":"typeIdentifier","preciseIdentifier":"s:14StripePayments39STPRedirectContextSourceCompletionBlocka","text":"STPRedirectContextSourceCompletionBlock"},{"kind":"text","text":")"}],"abstract":[{"type":"text","text":"Initializer for context from an "},{"type":"codeVoice","code":"STPSource"},{"text":".","type":"text"}],"title":"init(source:completion:)","kind":"symbol","type":"topic","role":"symbol","url":"\/documentation\/stripe\/stpredirectcontext\/init(source:completion:)"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContext":{"kind":"symbol","url":"\/documentation\/stripe\/stpredirectcontext","fragments":[{"kind":"keyword","text":"class"},{"text":" ","kind":"text"},{"text":"STPRedirectContext","kind":"identifier"}],"abstract":[{"type":"text","text":"This is a helper class for handling redirects associated with STPSource and"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"type":"text","text":" "},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"type":"text","text":" "},{"type":"text","text":"then choose a redirect method. The context will fire the completion handler"},{"text":" ","type":"text"},{"type":"text","text":"when the redirect completes."},{"text":" ","type":"text"},{"text":"Due to the nature of iOS, very little concrete information can be gained","type":"text"},{"text":" ","type":"text"},{"text":"during this process, as all actions take place in either the Safari app","type":"text"},{"type":"text","text":" "},{"type":"text","text":"or the sandboxed SFSafariViewController class. The context attempts to"},{"text":" ","type":"text"},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"text":" ","type":"text"},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"text":" ","type":"text"},{"text":"However, it is possible the when the redirect is “completed”, the user may","type":"text"},{"text":" ","type":"text"},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"type":"text","text":" "},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"type":"text","text":", as a way"},{"text":" ","type":"text"},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"type":"text","text":" "},{"text":"was successful. Use Stripe webhooks on your backend server to listen for Source","type":"text"},{"type":"text","text":" "},{"type":"text","text":"state changes and to make the charge."}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContext","title":"STPRedirectContext","role":"symbol","type":"topic","navigatorTitle":[{"text":"STPRedirectContext","kind":"identifier"}]},"doc://com.stripe.stripe-ios/documentation/Stripe/STPRedirectContextSourceCompletionBlock":{"title":"STPRedirectContextSourceCompletionBlock","type":"topic","url":"\/documentation\/stripe\/stpredirectcontextsourcecompletionblock","role":"symbol","navigatorTitle":[{"kind":"identifier","text":"STPRedirectContextSourceCompletionBlock"}],"kind":"symbol","abstract":[{"type":"text","text":"A callback that is executed when the context believes the redirect action has been completed."}],"fragments":[{"text":"typealias","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPRedirectContextSourceCompletionBlock","kind":"identifier"}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPRedirectContextSourceCompletionBlock"},"doc://com.stripe.stripe-ios/documentation/Stripe/STPSource":{"url":"\/documentation\/stripe\/stpsource","role":"symbol","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"STPSource","kind":"identifier"}],"abstract":[{"type":"text","text":"Representation of a customer’s payment instrument created with the Stripe API. - seealso: https:\/\/stripe.com\/docs\/api#sources"}],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPSource","type":"topic","navigatorTitle":[{"text":"STPSource","kind":"identifier"}],"kind":"symbol","title":"STPSource"},"doc://com.stripe.stripe-ios/documentation/Stripe":{"type":"topic","title":"Stripe","url":"\/documentation\/stripe","abstract":[],"identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe","kind":"symbol","role":"collection"}}}