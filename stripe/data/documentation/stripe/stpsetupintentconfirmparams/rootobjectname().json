."},{"type":"text","text":" "},{"text":"Instead of passing this to ","type":"text"},{"code":"STPAPIClient.confirmSetupIntent(...)","type":"codeVoice"},{"type":"text","text":" directly, we recommend using "},{"code":"STPPaymentHandler","type":"codeVoice"},{"type":"text","text":" to handle any additional steps for you."}],"title":"STPSetupIntentConfirmParams","url":"\/documentation\/stripe\/stpsetupintentconfirmparams","fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"STPSetupIntentConfirmParams"}],"kind":"symbol","identifier":"doc:\/\/com.stripe.stripe-ios\/documentation\/Stripe\/STPSetupIntentConfirmParams","type":"topic","navigatorTitle":[{"kind":"identifier","text":"STPSetupIntentConfirmParams"}],"role":"symbol"}}}