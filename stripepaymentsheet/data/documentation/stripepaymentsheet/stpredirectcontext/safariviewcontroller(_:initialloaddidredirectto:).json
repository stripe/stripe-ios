t":[{"text":"Inherited from ","type":"text"},{"code":"SFSafariViewControllerDelegate.safariViewController(_:initialLoadDidRedirectTo:)","type":"codeVoice"},{"text":".","type":"text"}],"hierarchy":{"paths":[["doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet","doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext","doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext\/SFSafariViewControllerDelegate-Implementations"]]},"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext\/safariViewController(_:initialLoadDidRedirectTo:)"},"schemaVersion":{"major":0,"patch":0,"minor":3},"references":{"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet/STPRedirectContext/safariViewController(_:initialLoadDidRedirectTo:)":{"title":"safariViewController(_:initialLoadDidRedirectTo:)","abstract":[],"identifier":"doc:\/\/com.stripe.stripe-payment-sheet\/documentation\/StripePaymentSheet\/STPRedirectContext\/safariViewController(_:initialLoadDidRedirectTo:)","role":"symbol","type":"topic","url":"\/documentation\/stripepaymentsheet\/stpredirectcontext\/safariviewcontroller(_:initialloaddidredirectto:)","kind":"symbol","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"text":"safariViewController","kind":"identifier"},{"text":"(","kind":"text"},{"preciseIdentifier":"c:objc(cs)SFSafariViewController","kind":"typeIdentifier","text":"SFSafariViewController"},{"text":", ","kind":"text"},{"kind":"externalParam","text":"initialLoadDidRedirectTo"},{"kind":"text","text":": "},{"text":"URL","preciseIdentifier":"s:10Foundation3URLV","kind":"typeIdentifier"},{"text":")","kind":"text"}]},"doc://com.stripe.stripe-payment-sheet/documentation/StripePaymentSheet/STPRedirectContext":{"abstract":[{"text":"This is a helper class for handling redirects associated with STPSource and","type":"text"},{"type":"text","text":" "},{"type":"text","text":"STPPaymentIntents."},{"text":" ","type":"text"},{"type":"text","text":"Init and retain an instance with the Source or PaymentIntent you want to handle,"},{"type":"text","text":" "},{"type":"text","text":"then choose a redirect method. The context will fire the completion handler"},{"text":" ","type":"text"},{"type":"text","text":"when the redirect completes."},{"text":" ","type":"text"},{"type":"text","text":"Due to the nature of iOS, very little concrete information can be gained"},{"text":" ","type":"text"},{"type":"text","text":"during this process, as all actions take place in either the Safari app"},{"text":" ","type":"text"},{"text":"or the sandboxed SFSafariViewController class. The context attempts to","type":"text"},{"type":"text","text":" "},{"text":"detect when the user has completed the necessary redirect action by listening","type":"text"},{"type":"text","text":" "},{"text":"for both app foregrounds and url callbacks received in the app delegate.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"However, it is possible the when the redirect is “completed”, the user may"},{"type":"text","text":" "},{"text":"have not actually completed the necessary actions to authorize the charge.","type":"text"},{"type":"text","text":" "},{"text":"You should not use either this class, nor ","type":"text"},{"type":"codeVoice","code":"STPAPIClient"},{"type":"text","text":", as a way"},{"type":"text","text":" "},{"type":"text","text":"to determine when you should charge the Source or to determine if the redirect"},{"text":" ","type":"text"},{"type":"text","text":"was successful. Use Stripe webhooks on your backend server to listen for Source"},{"text":" ","type":"text"},{"type":"text","text":"state changes and to make the charge."}],"role":"symbol","kind":"symbol","url":"\/documentation\/stripepaymentsheet\/stpredirectcontext","navigatorTitle":[{"kind":"identifier","text":"STPRedirect