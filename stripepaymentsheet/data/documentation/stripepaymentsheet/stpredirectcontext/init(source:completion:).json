ent":[{"text":"completion: A block to fire when the action is believed to have","type":"text"},{"type":"text","text":" "},{"text":"been completed.","type":"text"}],"type":"paragraph"}]}]}]},{"content":[{"inlineContent":[{"type":"text","text":"Returns: nil if the specified source is not a redirect-flow source. Otherwise"},{"type":"text","text":" "},{"text":"a new context object.","type":"text"}],"type":"paragraph"}]}]}],"name":"Note"},{"name":"Note","style":"note","type":"aside","content":[{"type":"paragraph","inlineContent":[{"text":"Execution of the completion block does not necessarily mean the user","type":"text"},{"type":"text","text":" "},{"text":"successfully performed the redirect action. You should listen for source status","type":"text"},{"type":"text","text":" "},{"type":"text","text":"change webhooks on your backend to determine the result of a redirect."}]}]}],"kind":"content"}],"sections":[],"abstract":[{"type":"text","text":"Initializer for context from an "},{"code":"STPSource","type":"codeVoice"},{"type":"text","text":"."}],"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/stripepaymentsheet\/stpredirectcontext\/init(source:completion:)"]}],"schemaVersion":{"patch":0,"major":0,"minor":3},"metadata":{"roleHeading":"Initializer","symbolKind":"init","fragments":[{"kind":"keyword","text":"convenience"},{"kind":"text","text":" "},{"text":"init","kind":"identifier"},{"text":"?(","kind":"text"},{"kind":"externalParam","text":"source"},{"text":": ","kind":"text"},{"text":"STPSource","preciseIdentifier":"c:@M@StripePayments@objc(cs)STPSource","kind":"typeIdentifier"},{"text":", ","kind":"text"},{"text":"completion","kind":"externalParam"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:14StripePayments39STPRedirectContextSourceCompletionBlocka","kind":"typeIdentifier","text":"STPRedirectContextSourceCompletionBlock"},{"kind":"text","text":")"}],"role":"symbol","externalID":"c:@M@StripePayments@objc(cs)STPRedirectContext(im)i