# See: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[VERSION]
insert_final_newline = false

[{*.rb,*.yml,*.yaml}]
indent_style = space
indent_size = 2

[{*.swift}]
indent_style = space
indent_size = 4
